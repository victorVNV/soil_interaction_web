@article{Xavier2007,
  title = {A Study on the Feeding Habits and Gnathal Appendages in Oribatid Mites ({{Acarina}}: {{Cryptostigmata}})},
  author = {Xavier, Alphonsa and Haq, M.A.},
  year = {2007},
  journal = {ZOOS' PRINT JOURNAL},
  volume = {22},
  number = {5},
  pages = {2671--2674},
  urldate = {2022-04-27},
  abstract = {Gut contents of eight species of oribatid mites collected from the field were analysed employing suitable staining techniques, which showed the presence of bacteria, fungal hyphae and spores, pollen grains, leaf and wood particles in various stages of digestion. Based on this, the species considered could be placed under different trophic niches. The pedipalp, chelicera and rutellum which formed the major grasping and masticating appendages of these mites were subjected to detailed structural analysis which established their functional role in food grasping, collection and mastication.}
}
