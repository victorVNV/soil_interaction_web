@article{Sadaka-Laulan1998,
  title = {Feeding Preferences of the Collembolan {{Onychiurus}} Sinensis for Fungi Colonizing Holm Oak Litter ({{Quercus}} Rotundifolia {{Lam}}.)},
  author = {{Sadaka-Laulan}, Nassima and Ponge, Jean-Fran{\c c}ois and Roquebert, Marie-France and Bury, Edith and Boumezzough, Ali},
  year = {1998},
  month = sep,
  journal = {European Journal of Soil Biology},
  volume = {34},
  number = {4},
  pages = {179--188},
  issn = {1164-5563},
  doi = {10.1016/S1164-5563(00)86660-1},
  urldate = {2022-04-27},
  abstract = {Ten fungi isolated from decaying holm oak leaves (Quercus rotundifolia Lam.) have been presented to the collembolan species Onychiurus sinensis (Hexapoda). The attractiveness and selectivity of the ten fungi was investigated taken into account the effect of the substrate on which the fungus was growing (malt-agar or litter) and the effect of fungal odour. Furthermore, moulting, growth, survival and reproduction of O. sinensis in the presence of each of the ten test fungi were studied. Mucor plumbeus and Trichothecium roseum were the most preferred whatever the culture substrate and their odour was the most attractive. Two fungi (Penicillium spinulosum and the Basidiomycete S41) attracted Collembola by their odour but were not preferred as food sources. The animals could survive and reproduce on a mono-diet of several of our test fungi, but not on the Basidiomycete S41 and on Trichoderma polysporum. R\'esum\'e Dix souches fongiques isol\'ees de la liti\`ere de ch\^ene vert (Quercus rotundifolia) sont utilis\'ees comme source de nourriture pour le collembole Onychiurus sinensis. L'attrait et la s\'electivit\'e de chaque souche sont \'etudi\'es en fonction du substrat de croissance (malt-agar ou liti\`ere) et de l'odeur du champignon. Parmi les souches test\'ees, Mucor plumbeus et Trichothecium roseum sont les pr\'ef\'er\'ees quel que soit le substrat de culture, et leurs odeurs les mieux per\c{c}ues. En revanche, Penicillium spinulosum et le Basidiomycete S41 attirent les collemboles par leur odeur, mais sont peu consomm\'es. L'impact de chacune des souches sur les param\`etres biologiques du collembole est significatif. La plupart d'entre elles permettent aux individus non seulement de se maintenir, mais aussi de se d\'evelopper et de se reproduire, \`a l'exception du Basidiomycete S41 et de Trichoderma polysporum.},
  langid = {english},
  keywords = {champignon,Collembola,Collemboles,feeding preferences,fungal odour,fungi,leaf litter,liti\`ere,odeur de champignon,pr\'ef\'erences alimentaires}
}

