@article{Koehler1997,
  title = {Mesostigmata ({{Gamasina}}, {{Uropodina}}), Efficient Predators in Agroecosystems},
  author = {Koehler, Hartmut H.},
  year = {1997},
  month = apr,
  journal = {Agriculture, Ecosystems \& Environment},
