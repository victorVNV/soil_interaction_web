@article{Walter1988a,
  title = {Nematophagy by Soil Arthropods from the Shortgrass Steppe, {{Chihuahuan}} Desert and {{Rocky Mountains}} of the Central {{United States}}},
  author = {Walter, David Evans},
  year = {1988},
  month = nov,
  journal = {Agriculture, Ecosystems \& Environment},
  series = {Proceedings of a {{Workshop}} on {{Interactions Between Soil-Inhabiting Invertebrates}} and {{Microorganisms}} in {{Relation}} to {{Plant Growth}}},
  volume = {24},
  number = {1},
  pages = {307--316},
  issn = {0167-8809},
  doi = {10.1016/0167-8809(88)90074-6},
  urldate = {2022-04-27},
  abstract = {Nematodes are an important food resource for many species of soil arthropods. At an intensively studied site in the shortgrass steppe of western Nebraska, at least 33 species of arthropods fed on nematodes. Observations for soil arthopods from other shortgrass steppe sites in Colorado, Wyoming and New Mexico, from Chihuahuan desert sites in New Mexico and Texas and from montane litter habitats in Colorado indicate similar patterns of nematophagy. Nematophages include species of parasitiform mites in the families Zerconidae, Ologamasidae, Parasitidae, Laelapidae, Ascidae, Rhodacaridae, Macrochelidae, Eviphididae, Phytoseiidae and Veigaiidae; and acariform mites in the families Alycidae, Alicorhagiidae, Tydeidae, Eupodidae, Paratydeidae, Acaridae, Galumnidae, Haplozetidae and Ceratozetidae. Families of prostigmatid mites traditionally considered predatory are restricted to arthropod prey, and do not feed on nematodes. Some species of Collembola, Coleoptera, Symphyla and possibly small centipedes also feed on nematodes. Nematophagous arthropods fall into 3 trophic classes: (1) omnivores which feed on a variety of algae and fungi as well as on nematodes; (2) general predators of soil invertebrates; (3) nematode specialists. Some omnivores and general predators develop more rapidly and produce more offspring on a diet of nematodes than on other foods. Some mesostigmatid mites are small (allowing access to small pore spaces), have relatively rapid generation times and high reproductive outputs, prefer nematode prey (but can utilize alternative food sources) and are easily cultured. These animals have potential for development as biological-control agents for plant-parasitic nematodes.},
  langid = {english}
}

