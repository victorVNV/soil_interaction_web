  keywords = {decomposer animals,feeding guilds,gamasid mites,Graffenrieda emarginata,oribatid mites,{$\delta$}15N}
}

@article{Imbriani1983,
  title = {Studies on {{Lasioseius}} Scapulatus, a {{Mesostigmatid}} Mite Predaceous on Nematodes},
  author = {Imbriani, I. and Mankau, R.},
  year = {1983},
  month = oct,
  journal = {Journal of Nematology},
  volume = {15},
  number = {4},
  pages = {523--528},
  issn = {0022-300X},
  urldate = {2022-04-27},
  abstract = {The life history and feeding habits of Lasioseius scapulatus, an ascid predator and potential biocontrol agent of nematodes, was examined. Reproduction was asexual, and the life cycle was 8-10 days at room temperature. Life history consisted of the egg, protonymph, deutonymph, and adult. Both nymphal stages and the adult captured and consumed nematodes. Two fungal genera and eight genera of nematodes were suitable food sources. Second-stage root-knot nematode juveniles were eaten, but eggs and adult females were not. The mite fed voraciously on nematodes and drastically reduced Aphelenchus avenae populations in vitro. It is suggested that mites are of considerable importance in the ecology of certain nematodes.},
  pmcid = {PMC2618325},
  pmid = {19295841}
}

