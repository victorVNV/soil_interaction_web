}

@article{Illig2005,
  title = {Where Are the Decomposers? {{Uncovering}} the Soil Food Web of a Tropical Montane Rain Forest in Southern {{Ecuador}} Using Stable Isotopes ({{15N}})},
  shorttitle = {Where Are the Decomposers?},
  author = {Illig, Jens and Langel, Reinhard and Norton, Roy A. and Scheu, Stefan and Maraun, Mark},
  year = {2005},
  month = sep,
  journal = {Journal of Tropical Ecology},
  volume = {21},
